# Configuration NextJS CLI
# Copiez ce fichier vers .env et modifiez les valeurs

# Mode de développement (true/false)
DEBUG=false

# Utiliser le fichier de configuration JSON (true/false)
USE_CONFIG_FILE=true

# Tokens GitHub pour les tests (remplacez par vos vrais tokens)
GITHUB_TOKEN_ADMIN=ghp_your_private_token_here
GITHUB_TOKEN_DEVELOPER=ghp_another_private_token

# URLs des dépôts par défaut
REPO_CLEAN_URL=https://github.com/vercel/next.js/tree/canary/examples/hello-world
REPO_ADVANCED_URL=https://github.com/your-org/nextjs-advanced-template

# Timeout pour les requêtes HTTP (en millisecondes)
HTTP_TIMEOUT=5000
