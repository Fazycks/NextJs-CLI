{
    "name": "@fazycks/nextjs-template-cli",
    "version": "1.0.0",
    "description": "CLI pour cloner des dépôts NextJS avec système d'authentification et ajout de composants",
    "main": "dist/index.js",
    "bin": {
        "nextjs-cli": "./dist/index.js"
    },
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node src/index.ts",
        "watch": "tsc --watch",
        "prepublishOnly": "npm run build",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "cli",
        "nextjs",
        "github",
        "typescript",
        "components",
        "templates"
    ],
    "author": "Fazycks <fazycks@example.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fazycks/nextjs-template-cli.git"
    },
    "bugs": {
        "url": "https://github.com/fazycks/nextjs-template-cli/issues"
    },
    "homepage": "https://github.com/fazycks/nextjs-template-cli#readme",
    "files": [
        "dist",
        "config.json",
        "README.md",
        "LICENSE"
    ],
    "engines": {
        "node": ">=16.0.0"
    },
    "dependencies": {
        "commander": "^11.1.0",
        "inquirer": "^9.2.12",
        "simple-git": "^3.20.0",
        "chalk": "^4.1.2",
        "ora": "^5.4.1",
        "fs-extra": "^11.2.0",
        "axios": "^1.6.0"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@types/inquirer": "^9.0.7",
        "@types/fs-extra": "^11.0.4",
        "typescript": "^5.3.0",
        "ts-node": "^10.9.0"
    }
